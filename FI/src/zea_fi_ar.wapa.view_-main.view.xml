<mvc:View controllerName="sync.e17.sync.e17.controller.Main"                                                                                                                                                                                                   
xmlns:mvc="sap.ui.core.mvc" displayBlock="true"                                                                                                                                                                                                                
xmlns="sap.m"                                                                                                                                                                                                                                                  
xmlns:layout="sap.ui.layout"                                                                                                                                                                                                                                   
xmlns:viz="sap.viz.ui5.controls"&nbsp;                                                                                                                                                                                                                         
xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"                                                                                                                                                                                                            
xmlns:viz.data="sap.viz.ui5.data"                                                                                                                                                                                                                              
xmlns:cm="sap.suite.ui.commons"                                                                                                                                                                                                                                
height="100%">                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
<Page id="page" title="{i18n>title}">                                                                                                                                                                                                                          
    <content>                                                                                                                                                                                                                                                  
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                               
        <layout:FixFlex&nbsp;                                                                                                                                                                                                                                  
            id="idFixFlex"&nbsp;                                                                                                                                                                                                                               
            minFlexSize="500">                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
            <layout:fixContent>                                                                                                                                                                                                                                
                <Text text="고객사별 매출채권 조회" />                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
            </layout:fixContent>                                                                                                                                                                                                                               
            <layout:fixContent >                                                                                                                                                                                                                               
                <cm:ChartContainer&nbsp;                                                                                                                                                                                                                       
                    class="sapUiResponsiveContentPadding"                                                                                                                                                                                                      
                    id="idChartContainer"                                                                                                                                                                                                                      
                    showFullScreen="true"                                                                                                                                                                                                                      
                    showLegend="true"                                                                                                                                                                                                                          
                    showLegendButton="true"                                                                                                                                                                                                                    
                    showZoom="true"                                                                                                                                                                                                                            
                    title="고객사별 채권조회 차트"                                                                                                                                                                                                                       
                    autoAdjustHeight="false"                                                                                                                                                                                                                   
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                       
                >                                                                                                                                                                                                                                              
                    <cm:content>                                                                                                                                                                                                                               
                        <cm:ChartContainerContent&nbsp;                                                                                                                                                                                                        
                            icon="sap-icon://bar-chart"                                                                                                                                                                                                        
                            title="차트"&nbsp;                                                                                                                                                                                                                   
                        >                                                                                                                                                                                                                                      
                            <cm:content>                                                                                                                                                                                                                       
                                <!-- 이 안에 차트를 그려줄 예정 -->                                                                                                                                                                                                       
                                <viz:Popover id="idPopOver"></viz:Popover>                                                                                                                                                                                     
                                <viz:VizFrame id="idVizFrame" uiConfig="{applicationSet:'fiori'}"                                                                                                                                                              
                                height='500px' width="100%" vizType='column'>                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
                                    <viz:dataset>                                                                                                                                                                                                              
                                        <viz.data:FlattenedDataset data="{/bseg002Set}">                                                                                                                                                                       
                                            <viz.data:dimensions>                                                                                                                                                                                              
                                                <viz.data:DimensionDefinition name="고객사" value="{Bpname}" />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                       
                                                <viz.data:DimensionDefinition name="고객코드" value="{Bpcode}" />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                      
                                                <viz.data:DimensionDefinition name="계정과목" value="{Saknr}" />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                       
                                            </viz.data:dimensions>                                                                                                                                                                                             
                                            <viz.data:measures>                                                                                                                                                                                                
                                                <viz.data:MeasureDefinition name="금액" value="{Wrbtr}" />                                                                                                                                                       
                                                <!-- <viz.data:MeasureDefinition name="통화키" value="{DWaers}" /> -->                                                                                                                                            
                                            </viz.data:measures>                                                                                                                                                                                               
                                        </viz.data:FlattenedDataset>                                                                                                                                                                                           
                                    </viz:dataset>                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
                                    <viz:feeds>                                                                                                                                                                                                                
                                        <viz.feeds:FeedItem id='valueAxisFeed'    uid="valueAxis"    type="Measure"   values="금액" />                                                                                                                           
                                        <viz.feeds:FeedItem id="categoryAxisFeed" uid="categoryAxis" type="Dimension" values="고객사,고객코드,계정과목" />                                                                                                                
                                        <!-- <viz.feeds:FeedItem uid="categoryAxis" type="Dimension" values="항공편" /> -->                                                                                                                                       
                                    </viz:feeds>                                                                                                                                                                                                               
                                </viz:VizFrame>                                                                                                                                                                                                                
                            </cm:content>                                                                                                                                                                                                                      
                        </cm:ChartContainerContent>                                                                                                                                                                                                            
                        <cm:ChartContainerContent                                                                                                                                                                                                              
                            icon="sap-icon://table-view"                                                                                                                                                                                                       
                            title="데이터"                                                                                                                                                                                                                        
                        >                                                                                                                                                                                                                                      
                            <cm:content>                                                                                                                                                                                                                       
                            <!-- 차트로 보여주기 -->                                                                                                                                                                                                                  
                                <Table items="{/bseg002Set}" >                                                                                                                                                                                                 
                                    <columns>                                                                                                                                                                                                                  
                                        <!-- <Column ><Label text="고객코드 &amp; 검수일자"/></Column> -->                                                                                                                                                             
                                        <Column ><Label text="고객코드"/></Column>                                                                                                                                                                                 
                                        <Column ><Label text="고객명"/></Column>                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
                                        <Column ><Label text="매출채권"/></Column>                                                                                                                                                                                 
                                        <Column ><Label text="금액"/></Column>                                                                                                                                                                                   
                                        <Column ><Label text="통화코드"/></Column>                                                                                                                                                                                 
                                        <!-- <Column ><Label text="최종 생산량"/></Column> -->                                                                                                                                                                      
                                        <!-- <Column ><Label text="폐기 사유"/></Column> -->                                                                                                                                                                       
                                    </columns>                                                                                                                                                                                                                 
                                    <items>                                                                                                                                                                                                                    
                                        <ColumnListItem >                                                                                                                                                                                                      
                                            <ObjectIdentifier title="{Bpcode}" />                                                                                                                                                                              
                                            <ObjectIdentifier text="{Bpname}" />                                                                                                                                                                               
                                            <!-- <ObjectNumber number="{path: 'Seatsmax', type: 'sap.ui.model.odata.type.Integer'}" unit="석" /> -->                                                                                                            
                                            <ObjectNumber number="{Saknr}"  />                                                                                                                                                                                 
                                            <ObjectNumber number="{Wrbtr}"  />                                                                                                                                                                                 
                                            <ObjectNumber number="{Waers}"  />                                                                                                                                                                                 
                                            <!-- <ObjectNumber number="{path:'Wrbtr',type:'sap.ui.model.odata.type.Currency'}"unit="KRW"  /> -->                                                                                                               
                                            <!-- <ObjectIdentifier text="{Defreason}"/> -->                                                                                                                                                                    
                                        </ColumnListItem>                                                                                                                                                                                                      
                                    </items>                                                                                                                                                                                                                   
                                </Table>                                                                                                                                                                                                                       
                            </cm:content>                                                                                                                                                                                                                      
                        </cm:ChartContainerContent>                                                                                                                                                                                                            
                    </cm:content>                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
                </cm:ChartContainer>                                                                                                                                                                                                                           
            </layout:fixContent>                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
        </layout:FixFlex>                                                                                                                                                                                                                                      
                                                                                                                                                                                                                                                               
    </content>                                                                                                                                                                                                                                                 
</Page>                                                                                                                                                                                                                                                        
</mvc:View>                                                                                                                                                                                                                                                    