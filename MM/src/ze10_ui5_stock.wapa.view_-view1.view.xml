<mvc:View controllerName="sync.ea.stock.controller.View1"                                                                                                                                                                                                      
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"                                                                                                                                                                                                            
    xmlns="sap.m"                                                                                                                                                                                                                                              
    xmlns:layout="sap.ui.layout"                                                                                                                                                                                                                               
    xmlns:viz="sap.viz.ui5.controls"&nbsp;                                                                                                                                                                                                                     
    xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"                                                                                                                                                                                                        
    xmlns:viz.data="sap.viz.ui5.data"                                                                                                                                                                                                                          
    xmlns:cm="sap.suite.ui.commons"                                                                                                                                                                                                                            
    height="100%">                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
    <Page id="page" title="{i18n>title}">                                                                                                                                                                                                                      
        <content>                                                                                                                                                                                                                                              
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                       
            <layout:FixFlex&nbsp;                                                                                                                                                                                                                              
                id="idFixFlex"&nbsp;                                                                                                                                                                                                                           
                minFlexSize="500">                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
                <layout:fixContent>                                                                                                                                                                                                                            
                    <Text text="(주) 한국타이어앤테크놀로지" />                                                                                                                                                                                                            
                </layout:fixContent>                                                                                                                                                                                                                           
                <layout:fixContent >                                                                                                                                                                                                                           
                    <cm:ChartContainer&nbsp;                                                                                                                                                                                                                   
                        class="sapUiResponsiveContentPadding"                                                                                                                                                                                                  
                        id="idChartContainer"                                                                                                                                                                                                                  
                        showFullScreen="true"                                                                                                                                                                                                                  
                        showLegend="true"                                                                                                                                                                                                                      
                        showLegendButton="true"                                                                                                                                                                                                                
                        showZoom="true"                                                                                                                                                                                                                        
                        title="각 지점별 재고수량 확인"                                                                                                                                                                                                                  
                        autoAdjustHeight="false"                                                                                                                                                                                                               
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                               
                    >                                                                                                                                                                                                                                          
                        <cm:content>                                                                                                                                                                                                                           
                            <cm:ChartContainerContent&nbsp;                                                                                                                                                                                                    
                                icon="sap-icon://bar-chart"                                                                                                                                                                                                    
                                title="차트"&nbsp;                                                                                                                                                                                                               
                            >                                                                                                                                                                                                                                  
                                <cm:content>                                                                                                                                                                                                                   
                                    <!-- 이 안에 차트를 그려줄 예정 -->                                                                                                                                                                                                   
                                    <viz:Popover id="idPopOver"></viz:Popover>                                                                                                                                                                                 
                                    <viz:VizFrame id="idVizFrame" uiConfig="{applicationSet:'fiori'}"                                                                                                                                                          
                                        height='500px' width="100%" vizType='bar' >                                                                                                                                                                            
                                        <viz:dataset>                                                                                                                                                                                                          
                                            <viz.data:FlattenedDataset data="{/mms010Set}">                                                                                                                                                                    
                                                <viz.data:dimensions>                                                                                                                                                                                          
                                                    <viz.data:DimensionDefinition name="품목명" value="{Maktx}"/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                     
                                                    <viz.data:DimensionDefinition name="직영점명" value="{Pname1}"/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                   
                                                </viz.data:dimensions>                                                                                                                                                                                         
                                                <viz.data:measures>                                                                                                                                                                                            
                                                    <viz.data:MeasureDefinition name="재고수량" value="{Calqty}" />                                                                                                                                                
                                                </viz.data:measures>                                                                                                                                                                                           
                                            </viz.data:FlattenedDataset>                                                                                                                                                                                       
                                        </viz:dataset>                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
                                        <viz:feeds>                                                                                                                                                                                                            
                                            <viz.feeds:FeedItem id='valueAxisFeed'    uid="valueAxis"    type="Measure"   values="재고수량" />                                                                                                                     
                                            <!-- <viz.feeds:FeedItem id="categoryAxisFeed" uid="categoryAxis" type="Dimension" values="수량" /> -->                                                                                                              
                                            <viz.feeds:FeedItem uid="categoryAxis" type="Dimension" values="품목명,직영점명" />                                                                                                                                       
                                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
                                        </viz:feeds>                                                                                                                                                                                                           
                                    </viz:VizFrame>                                                                                                                                                                                                            
                                </cm:content>                                                                                                                                                                                                                  
                            </cm:ChartContainerContent>                                                                                                                                                                                                        
                            <cm:ChartContainerContent                                                                                                                                                                                                          
                                icon="sap-icon://table-view"                                                                                                                                                                                                   
                                title="재고현황"                                                                                                                                                                                                                   
                            >                                                                                                                                                                                                                                  
                                <cm:content>                                                                                                                                                                                                                   
                                    <!-- 이 안에는 차트에 사용된 데이터를 테이블로 보여줄 예정 -->                                                                                                                                                                                    
                                    <Table items="{/mms010Set}" >                                                                                                                                                                                              
                                        <columns>                                                                                                                                                                                                              
                                            <!-- <Column ><Label text="제품명 &amp; 직영점"/></Column> -->                                                                                                                                                           
                                            <Column ><Label text="직영점"/></Column>                                                                                                                                                                              
                                            <Column ><Label text="품목명"/></Column>                                                                                                                                                                              
                                            <Column ><Label text="재고수량"/></Column>                                                                                                                                                                             
                                        </columns>                                                                                                                                                                                                             
                                        <items>                                                                                                                                                                                                                
                                            <ColumnListItem >                                                                                                                                                                                                  
                                                <!-- <ObjectIdentifier title="{Maktx}"  text="{Pname1}"/> -->                                                                                                                                                  
                                                <ObjectIdentifier title="{Pname1}"  />                                                                                                                                                                         
                                                <ObjectIdentifier title="{Maktx}"  />                                                                                                                                                                          
                                                <!-- <ObjectIdentifier text="{}"/> -->                                                                                                                                                                         
                                                <ObjectNumber number="{Calqty}" unit="{Meins}" />                                                                                                                                                              
                                                <!-- <ObjectNumber number="{Seatsocc}" unit="석" /> -->                                                                                                                                                         
                                            </ColumnListItem>                                                                                                                                                                                                  
                                        </items>                                                                                                                                                                                                               
                                    </Table>                                                                                                                                                                                                                   
                                </cm:content>                                                                                                                                                                                                                  
                            </cm:ChartContainerContent>                                                                                                                                                                                                        
                        </cm:content>                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
                    </cm:ChartContainer>                                                                                                                                                                                                                       
                </layout:fixContent>                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
            </layout:FixFlex>                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
        </content>                                                                                                                                                                                                                                             
    </Page>                                                                                                                                                                                                                                                    
</mvc:View>                                                                                                                                                                                                                                                    