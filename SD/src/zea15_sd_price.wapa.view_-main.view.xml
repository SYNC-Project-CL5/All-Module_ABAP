<mvc:View controllerName="sync.ea.price.controller.Main"                                                                                                                                                                                                       
     xmlns:mvc="sap.ui.core.mvc" displayBlock="true"                                                                                                                                                                                                           
    xmlns="sap.m">                                                                                                                                                                                                                                             
    <Page id="page" title="{i18n>title}">                                                                                                                                                                                                                      
        <content>                                                                                                                                                                                                                                              
        <Panel headerText="타이어 판매 단가" expandable="false" expanded="true" height="auto" >                                                                                                                                                                       
            <Table&nbsp;                                                                                                                                                                                                                                       
                items="{/PriceSet}"                                                                                                                                                                                                                            
                >                                                                                                                                                                                                                                              
                <columns>                                                                                                                                                                                                                                      
                    <Column ><Label text="제품명" />    </Column>                                                                                                                                                                                                 
                    <Column ><Label text="유효기간 종료일" /></Column>                                                                                                                                                                                                
                    <Column ><Label text="유효기간 시작일" />  </Column>                                                                                                                                                                                              
                    <Column ><Label text="판매 단가" />  </Column>                                                                                                                                                                                                 
                    <Column ><Label text="삭제 플래그" />  </Column>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                        
                    <!-- <Column ><Label text="예상생산 시작일자" />  </Column> -->                                                                                                                                                                                    
                </columns>                                                                                                                                                                                                                                     
                <items>                                                                                                                                                                                                                                        
                    <ColumnListItem >                                                                                                                                                                                                                          
                        <cells>                                                                                                                                                                                                                                
                            <Text text="{Maktx}"/>                                                                                                                                                                                                             
                            <ObjectIdentifier  title="{                                                                                                                                                                                                        
                                path: 'ValidEn',                                                                                                                                                                                                               
                                type: 'sap.ui.model.odata.type.DateTime',                                                                                                                                                                                      
                                formatOptions:{                                                                                                                                                                                                                
                                    pattern: 'yyyy-MM-dd',                                                                                                                                                                                                     
                                    UTC: true                                                                                                                                                                                                                  
                                }                                                                                                                                                                                                                              
                            }"/>&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                             
                            <ObjectIdentifier  title="{                                                                                                                                                                                                        
                                path: 'ValidSt',                                                                                                                                                                                                               
                                type: 'sap.ui.model.odata.type.DateTime',                                                                                                                                                                                      
                                formatOptions:{                                                                                                                                                                                                                
                                    pattern: 'yyyy-MM-dd',                                                                                                                                                                                                     
                                    UTC: true                                                                                                                                                                                                                  
                                }                                                                                                                                                                                                                              
                            }"/>&nbsp;&nbsp;                                                                                                                                                                                                                   
                            <ObjectNumber&nbsp;                                                                                                                                                                                                                
                            number="{                                                                                                                                                                                                                          
                                parts:[{path:'Netpr'},{path:'Waers'}],                                                                                                                                                                                         
                                type: 'sap.ui.model.type.Currency',                                                                                                                                                                                            
                                formatOptions: {showMeasure: false}                                                                                                                                                                                            
                            }"                                                                                                                                                                                                                                 
                            unit="KRW"    />                                                                                                                                                                                                                   
                            <!-- <Text text="{Netpr} {Waers}"/> -->                                                                                                                                                                                            
                            <Text text="{Loekz}"/>                                                                                                                                                                                                             
                            <!-- <Text text="{Expsdate}"/> -->                                                                                                                                                                                                 
                        </cells>                                                                                                                                                                                                                               
                    </ColumnListItem>                                                                                                                                                                                                                          
                </items>                                                                                                                                                                                                                                       
            </Table>                                                                                                                                                                                                                                           
        </Panel>                                                                                                                                                                                                                                               
    </content>                                                                                                                                                                                                                                                 
</Page>                                                                                                                                                                                                                                                        
</mvc:View>                                                                                                                                                                                                                                                    